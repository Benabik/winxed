class OpAssignExpr : Expr
{
    var start;
    var eleft;
    var eright;
    function checkResult()
    {
        if (self.eleft.isidentifier())
            return self.eleft.checkResult();
        else
            InternalError('Unimplemented');
    }
    function optimize()
    {
        self.eleft = self.eleft.optimize();
        self.eright = self.eright.optimize();
        return self;
    }
    function emit(e, result)
    {
        string reg = self.emit_get(e);
        e.annotate(self.start);
        e.say('set ', result, ', ', reg);
    }
    function emit_get(e)
    {
        var eright = self.eright;
        string typeright = eright.checkResult();
        string rreg = eright.emit_get(e);
        var eleft = self.eleft;
        if (eleft instanceof MemberExpr || eleft instanceof IndexExpr) {
            eleft.emit_assign(e, typeright, rreg);
            return rreg;
        }
        else {
            string typeleft = eleft.checkResult();
            string lreg = eleft.emit_get(e);
            e.annotate(self.start);
            if (typeleft == 'P' && typeright != 'P')
                e.say('box ', lreg, ', ', rreg);
            else
                e.say('set ', lreg, ', ', rreg);
            return lreg;
        }
    }
}

