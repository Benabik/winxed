class IndexExpr : Expr
{
    var start;
    var left;
    var args;

    function checkResult() { return 'P'; }
    function parse(tk, owner, t, subexp)
    {
        self.owner = owner;
        self.start = t;
        self.left = subexp;
        self.args = new 'ResizablePMCArray';
        do {
            var arg = parseExpr(tk, owner);
            self.args.push(arg);
        } while ( (t= tk.get()).isop(','));
        if (! t.isop(']'))
            SyntaxError("Expected ']' or ',' in index expr", t);
        return self;
    }
    function emit(e, result)
    {
        if (! self.left.isidentifier())
            SyntaxError("Unimplemented", self.start);
        //int n = self.args;
        var regleft = self.left.getIdentifier();
        string argregs[];
        for (var arg in self.args) {
            string argreg = arg.emit_get(e);
            argregs.push(argreg);
        }
        e.print(result, ' = ', regleft, '[');
        string sep = '';
        for (string reg in argregs) {
            e.print(sep, reg);
            sep = ', ';
        }
        e.say(']');
    }
    function emit_assign(e, string typeright, string rreg)
    {
        var regleft = self.left.emit_get(e);
        string argregs[];
        for (var arg in self.args) {
            string argreg = arg.emit_get(e);
            argregs.push(argreg);
        }
        e.annotate(self.start);
        e.print(regleft, '[');
        string sep = '';
        for (string reg in argregs) {
            e.print(sep, reg);
            sep = ', ';
        }
        e.say('] = ', rreg);
    }
}

