function aux()
{
    var interp=getinterp();
    var outer= interp['continuation', 0];
    for (int i= 1; outer.caller() != null; ++i) {
        var ann= interp['annotations', i];
        string file= ann['file'];
        int line= ann['line'];
        say(outer, "\n\t", file, ': ', line);
        int j= i-1;
        outer= interp['continuation', i];
    }
}

function test()
{
    aux();
}

function main()
{
    test();
}
